{
  "name": "sayt",
  "version": "0.1.2",
  "description": "Client for the SAYT API.",
  "typings": "dist/index.d.ts",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "postinstall": "[ ! -d src ] || typings install",
    "test": "karma start",
    "ci": "karma start --singleRun=false",
    "typings:copy": "rimraf all_typings && cp -R typings all_typings && cp custom_typings/* all_typings",
    "docs": "rimraf docs && npm run typings:copy && typedoc --excludeExternals --includeDeclarations --includes all_typings/* --mode file --out docs src",
    "build": "rimraf dist && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/groupby/sayt.git"
  },
  "keywords": [
    "typescript",
    "library"
  ],
  "author": "Ben Teichman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/groupby/sayt/issues"
  },
  "homepage": "https://github.com/groupby/sayt#readme",
  "devDependencies": {
    "chai": "^3.2.0",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "object-assign": "^4.1.0",
    "phantomjs-prebuilt": "^2.1.7",
    "pre-commit": "^1.1.3",
    "rimraf": "^2.5.4",
    "sinon": "^1.16.1",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^0.8.2",
    "tslint-eslint-rules": "^1.3.0",
    "typedoc": "^0.4.5",
    "typescript": "^1.9.0-dev.20160615-1.0",
    "typings": "^1.3.3",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "es6-promise": "^3.1.2",
    "filter-object": "^2.1.0",
    "jsonp": "^0.2.0",
    "qs": "^6.2.0"
  }
}
